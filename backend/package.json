{
  "name": "graphqlStart",
  "version": "1.0.0",
  "description": "Boiler Plate for Node.js server with TypeScript, GraphQL and Mongo(Mongoose)",
  "main": "build/server/server.js",
  "scripts": {
    "start": "nodemon --exec ts-node server/server.ts",
    "start:local": "nodemon build/server/server.js",
    "tsc:watch": "tsc -w -p tsconfig.json",
    "restart": "rm -rf /node_modules && npm i",
    "lint": "eslint server/*.ts",
    "lint:fix": "eslint server/*.ts",
    "prettify": "prettier server/*.ts --write",
    "test": "jest",
    "oo": "echo Something"
  },
  "author": "Lazar Vasic <lazav94@gmail.com> ðŸ‰",
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "cors": "^2.8.5",
    "dd-trace": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-fields": "^2.0.3",
    "mongoose": "^5.9.9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/graphql-fields": "^1.3.2",
    "@types/jest": "^25.2.1",
    "@types/mongoose": "^5.7.11",
    "@types/node": "^13.11.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "Node.js",
    "TypeScript",
    "GraphQL",
    "Apollo",
    "Mongo",
    "Mongoose",
    "Jest",
    "React",
    "Next.js"
  ]
}
